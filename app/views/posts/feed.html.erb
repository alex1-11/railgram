<h1>Feed:</h1>

<div id="posts">
  <% @posts.each do |post| %>
    <% if post.present? %> 
      <%= render partial: 'posts/post', 
                 locals: { post:,
                           likes: @likes } %>
      <p>
        <%= link_to "Show this post", post_path(post) %>
      </p>
    <% end %> 
  <% end %>
</div>
